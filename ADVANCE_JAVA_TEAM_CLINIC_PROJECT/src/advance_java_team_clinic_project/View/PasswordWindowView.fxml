<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="passwordPane" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="advance_java_team_clinic_project.Controller.PasswordWindowController">
   <children>
      <PasswordField fx:id="currentPassword" layoutX="120.0" layoutY="67.0" prefHeight="25.0" prefWidth="180.0" promptText="Enter your current password" styleClass="editInputStyle" stylesheets="@style.css" />
      <PasswordField fx:id="passwordInput" layoutX="120.0" layoutY="113.0" prefHeight="25.0" prefWidth="180.0" promptText="Enter New Password" styleClass="editInputStyle" stylesheets="@style.css" />
      <PasswordField fx:id="passwordRepeatInput" layoutX="120.0" layoutY="165.0" prefHeight="25.0" prefWidth="180.0" promptText="Enter New Password again" styleClass="editInputStyle" stylesheets="@style.css" />
      <Button fx:id="submitBtn" layoutX="152.0" layoutY="222.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="117.0" styleClass="submitBtn" stylesheets="@style.css" text="Change Password" />
   </children>
</AnchorPane>
